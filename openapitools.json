{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 4,
  "generator-cli": {
    "version": "5.3.0",
    "generators": {
      "woocommerce-scala-akka-client": {
        "generatorName": "scala-akka",
        "output": "#{cwd}/woocommerce-akka-client",
        "glob": "src/main/resources/woocommerce-openapi-*.yml",
        "additionalProperties": {
          "mainPackage": "org.woocommerce.akkaclient",
          "artifactId": "woocommerce-akka-client",
          "groupId": "org.woocommerce"
        }
      }
    }
  }
}
